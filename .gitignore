# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

/node_modules
/yarn-error.log

.byebug_history
*.kate-swp

# Ignore the file for customizing the locale.
/config/locales/en_customize.yml
#Ignore production config
/config/keys/production/*

/vendor/*
/00_*
/01_*/*

2019/
.idea/
